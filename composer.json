{
	"name": "david63/mergeusers",
	"type": "phpbb-extension",
	"description": "Adds an ACP option to allow the merging of two users into one, with the main user taking ownership of all topics, attachments, PMs, etc. of both users.",
	"version": "3.1.0-RC1",
	"time": "2019-12-29",
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Extension converter"
		},
		{
			"name": "Jari Kanerva",
			"role": "Previous developer"
		},
		{
			"name": "Rich McGirr",
			"role": "Previous developer"
		},
		{
			"name": "Damian (a.k.a. Obsidian)",
			"role": "Previous developer"
		}],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "Merge Users",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0"
    	},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "merge_users_version_file.json",
			"ssl": false
    	}
	}
}
